.mb10 {
  margin-bottom: 10px;
}
.star {
  display: inline-block;
}
.star view {
  float: left;
  width: 20rpx;
  height: 20rpx;
  margin-left: 3px;
}
.star .full {
  background: url("data:image/png;base64,");
  background-size: 100%;
}
.star .empty {
  background: url("data:image/png;base64,");
  background-size: 100%;
}
.booking {
  font-size: 22rpx;
}
.booking .shop_detail {
  padding: 0 10px;
  padding-bottom: 10px;
  background-image: linear-gradient(90deg, #0af, #0085ff);
}
.booking .shop_detail .back {
  height: 30px;
  line-height: 30px;
  color: #fff;
}
.booking .shop_detail .back view:first-child {
  float: left;
  font-family: Consolas;
  font-size: 38rpx;
}
.booking .shop_detail .back view:last-child {
  float: right;
  font-size: 30rpx;
}
.booking .shop_detail .detail {
  position: relative;
}
.booking .shop_detail .detail image {
  width: 60px;
  height: 60px;
  position: absolute;
  left: 0;
}
.booking .shop_detail .detail .content {
  padding-left: 70px;
  width: 100%;
  box-sizing: border-box;
  color: #fff;
  display: flex;
  flex-wrap: wrap;
}
.booking .shop_detail .detail .content .title {
  font-size: 28rpx;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: 500;
}
.booking .shop_detail .detail .content .tips {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.booking .shop_detail .detail .content view:nth-child(2) {
  height: 30px;
  line-height: 30px;
}
.booking .shop_detail .detail > view:last-child {
  color: #fff;
  padding-top: 10px;
}
.booking .shop_detail .detail > view:last-child .activity {
  float: left;
}
.booking .shop_detail .detail > view:last-child .activity text {
  color: #fff;
  margin-right: 10px;
}
.booking .shop_detail .detail > view:last-child .number {
  float: right;
}
.booking .shop_detail .detail > view:last-child .number text {
  margin-left: 3px;
}
.booking .shop_detail .bg_black {
  padding: 50px 20px 0 20px;
  box-sizing: border-box;
  position: fixed;
  width: 100%;
  z-index: 11;
  top: 0;
  left: 0;
  background-color: #262626;
  color: #fff;
  text-align: center;
}
.booking .shop_detail .bg_black .title {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 44rpx;
  font-weight: 600;
  margin-bottom: 10px;
}
.booking .shop_detail .bg_black .star view {
  width: 30px !important;
  height: 30px !important;
}
.booking .shop_detail .bg_black .discount {
  position: relative;
  margin-top: 30px;
  padding-top: 30px;
  padding-bottom: 40px;
  border-top: 1px solid #ddd;
}
.booking .shop_detail .bg_black .discount > view:first-child {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  padding: 3px 5px;
  border-radius: 10px;
  height: 20px;
  line-height: 20px;
  border: 1px solid #ddd;
  z-index: 2;
  font-size: 28rpx;
  top: -14px;
  background-color: #262626;
}
.booking .shop_detail .bg_black .discount > view:last-child view {
  height: 20px;
  line-height: 20px;
  text-align: left;
  margin-bottom: 5px;
}
.booking .shop_detail .bg_black .discount > view:last-child view text {
  margin-right: 5px;
  padding: 2px;
  border-radius: 1px;
}
.booking .shop_detail .bg_black .tip {
  position: relative;
  padding-top: 30px;
  border-top: 1px solid #ddd;
}
.booking .shop_detail .bg_black .tip > view:first-child {
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  padding: 3px 5px;
  border-radius: 10px;
  height: 20px;
  line-height: 20px;
  top: -14px;
  border: 1px solid #ddd;
  z-index: 2;
  font-size: 28rpx;
  background-color: #262626;
}
.booking .shop_detail .bg_black .tip > view:last-child {
  font-size: 28rpx;
  text-align: left;
}
.booking .shop_detail .bg_black .close {
  position: absolute;
  bottom: 20px;
  left: 50%;
  transform: translate(-50%);
  border-radius: 50%;
  width: 50px;
  height: 50px;
  text-align: center;
  font-size: 60rpx;
  color: #ddd;
  border: 1px solid #ddd;
}
