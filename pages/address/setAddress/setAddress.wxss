.header {
  width: 100%;
  background-image: linear-gradient(90deg, #0af, #0085ff);
  color: #fff;
  height: 40px;
  line-height: 40px;
  position: relative;
  padding: 0 40px;
  box-sizing: border-box;
  text-align: center;
  font-size: 28rpx;
  font-weight: 600;
}
.header .goBack {
  position: absolute;
  top: 0;
  left: 0;
  height: 40px;
  width: 40px;
  padding-left: 10px;
  font-family: Consolas;
  font-size: 46rpx;
}
.header .space {
  position: absolute;
  top: 0;
  right: 0;
  height: 40px;
  width: 40px;
}
.setAddress {
  background-color: #f2f2f2;
  overflow: hidden;
}
.setAddress .input {
  background-color: #fff;
  width: 100%;
  padding: 10px 20px;
  box-sizing: border-box;
}
.setAddress .input .inner {
  width: 100%;
  position: relative;
}
.setAddress .input .inner .in {
  position: relative;
  width: 75%;
  display: inline-block;
}
.setAddress .input .inner input {
  width: 100%;
  background-color: #f4f4f4;
  padding-left: 30px;
  box-sizing: border-box;
  color: #999;
  height: 40px;
  line-height: 40px;
  font-size: 26rpx;
  display: inline-block;
}
.setAddress .input .inner button {
  width: calc(25% - 20px);
  margin-left: 20px;
  display: inline-block;
  height: 40px;
  line-height: 40px;
  background-color: #2395ff;
  color: #fff;
  padding-left: 5px;
  padding-right: 5px;
}
.setAddress .input .inner .icon {
  background: url("data:image/png;base64,");
  background-size: 100%;
  width: 16px;
  height: 16px;
  position: absolute;
  top: 12px;
  left: 10px;
}
.setAddress .input .inner .delete {
  position: absolute;
  right: 10px;
  top: 10px;
  font-size: 30rpx;
  color: #2395ff;
  z-index: 99;
}
.setAddress .location .local {
  padding: 10px 20px;
  color: #666;
}
.setAddress .location .again {
  padding: 20px;
  margin-top: 10px;
  background-color: #fff;
}
.setAddress .location .again view:first-child {
  float: left;
  font-weight: 600;
  font-size: 30rpx;
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.setAddress .location .again view:last-child {
  float: right;
  color: #2395ff;
  font-size: 26rpx;
}
.setAddress .address {
  background-color: #fff;
}
.setAddress .address .single {
  border-bottom: 1px solid #eee;
  padding: 10px 20px;
}
.setAddress .address .single view:first-child {
  font-size: 30rpx;
  color: #333;
  font-weight: 700;
}
.setAddress .address .single view:last-child {
  color: #333;
  font-size: 26rpx;
}
.setAddress .address .single:last-child {
  border-bottom: 0;
}
.setAddress .nothing {
  background-color: #f2f2f2;
  padding-top: calc((100% - 103px)/2);
  box-sizing: border-box;
}
.setAddress .nothing image {
  margin: 0 auto;
  display: block;
}
.setAddress .nothing view {
  width: 100%;
  text-align: center;
}
.setAddress .nothing view:first-child {
  color: #6a6a6a;
  font-size: 28rpx;
}
.setAddress .nothing view:last-child {
  color: #999;
  font-size: 24rpx;
}
