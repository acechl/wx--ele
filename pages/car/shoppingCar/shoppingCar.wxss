.mb10 {
  margin-bottom: 10px;
}
.header {
  width: 100%;
  background-image: linear-gradient(90deg, #0af, #0085ff);
  color: #fff;
  height: 40px;
  line-height: 40px;
  position: relative;
  padding: 0 40px;
  box-sizing: border-box;
  text-align: center;
  font-size: 36rpx;
  font-weight: 600;
}
.header .goBack {
  position: absolute;
  top: 0;
  left: 0;
  height: 40px;
  width: 40px;
  padding-left: 10px;
  -webkit-font-family: Consolas;
  font-size: 46rpx;
}
.header .space {
  position: absolute;
  top: 0;
  right: 0;
  height: 40px;
  width: 40px;
}
.wx_toast {
  padding: 5px 10px;
  line-height: 30px;
  border-radius: 20px;
  background-color: rgba(0, 0, 0, 0.6);
  color: #fff;
}
.shoppingCar {
  background-color: #f5f5f5;
  padding: 0 10px;
}
.shoppingCar .address {
  background-color: #fff;
  margin-bottom: 10px;
  height: 70px;
  position: relative;
}
.shoppingCar .address .adding {
  float: left;
  margin-left: 10px;
  line-height: 70px;
}
.shoppingCar .address .something {
  float: left;
  margin-left: 10px;
  padding: 10px;
  box-sizing: border-box;
}
.shoppingCar .address .something .name {
  display: inline-flex;
  align-items: flex-end;
  margin-bottom: 10px;
}
.shoppingCar .address .something .name .na {
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
}
.shoppingCar .address .something .name .phone {
  font-size: 24rpx;
  color: #333;
  margin-left: 5px;
}
.shoppingCar .address .something .place .location {
  color: #bbb;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.shoppingCar .address .something .place .babel {
  background-color: #3190e8;
  color: #fff;
  padding: 2px 4px;
  margin-right: 10px;
}
.shoppingCar .address .enter {
  float: right;
  margin-right: 10px;
  line-height: 70px;
  -webkit-font-family: Consolas;
  width: 30px;
  text-align: center;
}
.shoppingCar .address .bg-i {
  height: 5px;
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  background: url("data:image/png;base64,");
}
.shoppingCar .time {
  background-color: #fff;
  margin-bottom: 10px;
  padding: 0 10px;
  box-sizing: border-box;
  height: 40px;
}
.shoppingCar .time view:first-child {
  float: left;
  line-height: 40px;
}
.shoppingCar .time .picker {
  float: right;
}
.shoppingCar .time .picker view {
  line-height: 40px;
}
.shoppingCar .booking .single {
  background-color: #fff;
  padding: 0 10px;
  padding-top: 20px;
  position: relative;
  border-bottom: 1px solid #ddd;
  margin-bottom: 10px;
  box-sizing: border-box;
  margin-top: 10px;
}
.shoppingCar .booking .single .title {
  position: relative;
  margin-bottom: 10px;
}
.shoppingCar .booking .single .title .line {
  background-color: #e5e5e5;
  height: 2px;
}
.shoppingCar .booking .single .title .contents {
  position: absolute;
  left: 50%;
  background-color: #fff;
  padding: 0 10px;
  transform: translateX(-50%);
  top: -7px;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 30rpx;
  font-weight: 600;
}
.shoppingCar .booking .single .bottom {
  position: relative;
}
.shoppingCar .booking .single .bottom checkbox-group {
  position: absolute;
  left: 10px;
  top: 27px;
}
.shoppingCar .booking .single .bottom .content {
  padding-left: 50px;
  width: 100%;
  font-size: 30rpx;
  box-sizing: border-box;
}
.shoppingCar .booking .single .bottom .content .menu {
  border-bottom: 1px solid #ddd;
}
.shoppingCar .booking .single .bottom .content .first {
  height: 70px;
  line-height: 70px;
}
.shoppingCar .booking .single .bottom .content .first view:first-child {
  float: left;
}
.shoppingCar .booking .single .bottom .content .first view:last-child {
  float: right;
}
.shoppingCar .booking .single .bottom .content .first view:last-child text:first-child {
  margin-right: 20rpx;
}
.shoppingCar .booking .single .bottom .content .least {
  height: 40px;
  line-height: 40px;
}
.shoppingCar .booking .single .bottom .content .least view:first-child {
  float: left;
}
.shoppingCar .booking .single .bottom .content .least view:last-child {
  float: right;
}
.shoppingCar .booking .single .bottom .content .all {
  height: 40px;
  line-height: 40px;
}
.shoppingCar .booking .single .bottom .content .all view:first-child {
  float: left;
}
.shoppingCar .booking .single .bottom .content .all view:last-child {
  float: right;
}
.shoppingCar .detail {
  padding: 0 10px;
  background-color: #fff;
}
.shoppingCar .detail .pay {
  height: 40px;
  line-height: 40px;
  font-size: 35rpx;
  font-weight: 600;
  border-bottom: 1px solid #ddd;
}
.shoppingCar .detail .taste {
  height: 40px;
  line-height: 40px;
}
.shoppingCar .detail .taste view:first-child {
  float: left;
  font-size: 35rpx;
  font-weight: 600;
}
.shoppingCar .detail .taste view:last-child {
  float: right;
  color: #999;
  -webkit-font-family: Consolas;
  width: 30px;
  text-align: right;
  font-size: 40rpx;
}
.shoppingCar .detail .taste .remark {
  font-size: 24rpx !important;
  color: #333;
  width: calc(100% - 40px);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.shoppingCar .middle {
  padding-bottom: 60px;
  padding-top: 50px;
}
.shoppingCar .goPay {
  position: fixed;
  padding: 0 10px;
  bottom: 0;
  width: 100%;
  height: 50px;
  line-height: 50px;
  left: 0;
  background-color: rgba(0, 0, 0, 0.9);
}
.shoppingCar .goPay view:first-child {
  float: left;
  color: #fff;
  font-size: 40rpx;
}
.shoppingCar .goPay view:last-child {
  float: right;
  background-color: #00d762;
  color: #fff;
  text-align: center;
  width: 100px;
  font-size: 30rpx;
}
